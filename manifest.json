{
    "manifest_version": 3,
    "name": "Belbe, Corrupted Spelltable Observer",
    "description": "Extension intended to observe and record the state of current active Spelltable game.",
    "version": "0.1",
    "icons": {
        "16":"icons/belbe_icon_16.png",
        "32":"icons/belbe_icon_32.png",
        "48":"icons/belbe_icon_48.png",
        "128":"icons/belbe_icon_128.png"
    },
    "action": {
      "default_popup": "popups/index.html",
      "default_icon": "icons/belbe_icon_128.png"
    },
    "permissions": ["scripting", "activeTab"],
    "background":{}
  }
  